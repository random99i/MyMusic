<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yc.mp.mapper.mUserMapper">

	<insert id="addmUser" parameterType="mUser">
		insert into mUser(usid,utel,upwd) values(sq_usid.nextval,#{utel},#{upwd}) 
	</insert>
	
	<select id="selectmUser" parameterType="mUser" resultType="mUser">
		select * from mUser where utel = #{utel} and upwd = #{upwd}
	</select>
</mapper>